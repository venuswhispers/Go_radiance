type Block struct {
  kind      Int
  slot      Int
  entries   [ Link ]
  shredding [ Shredding ]
} representation tuple

type Shredding struct {
  entryEndIdx Int
  shredEndIdx Int
} representation tuple

type Entry struct {
  kind       Int
  numHashes  Int
  hash       Hash
  txs        TransactionList
  txMetas    TransactionMetaList
} representation tuple

type Hash bytes

type TransactionList [ Link ]

type TransactionMetaList [ Link ]

type Transaction bytes
