type Block struct {
  slot      Int
  entries   [ Link ]
  shredding [ Shredding ]
} representation tuple

type Shredding struct {
  entryEndIdx Int
  shredEndIdx Int
} representation tuple

type Entry struct {
  slot Int
  idx  Int

  numHashes  Int
  hash       Hash
  txs        TransactionList
} representation tuple

type Hash bytes

type TransactionList [ Link ]

type Transaction bytes
